div
  div
    span See
    div(x-ng-click="addLink();")
      i.fa.fa-plus
  ul
    li(x-ng-repeat="see in endpoint.metadata.sees | orderBy:['seeOrder', 'title']")
      i(data-tribe-editable-link, data-href="see.href", data-title="see.title")
      div(x-ng-click="removeLink(see);")
        i.fa.fa-trash
